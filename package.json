{
  "name": "aframe-locomotion",
  "version": "0.2.0",
  "description": "A-Frame components for smooth locomotion and snap turning",
  "module": "dist/aframe-locomotion.esm.min.js",
  "browser": "dist/aframe-locomotion.umd.min.js",
  "main": "dist/aframe-locomotion.esm.min.js",
  "types": "dist/aframe-locomotion.d.ts",
  "author": "Noeri Huisman",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"rollup -c rollup.config.dev.js -w\" \"live-server --port=4000 --no-browser ./dev --mount=/js:./dist\"",
