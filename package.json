    "docs": "rimraf docs/reference/ && aframe-typescript-docs-generator -o docs/reference/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrxz/aframe-locomotion"
  },
  "bugs": {
    "url": "https://github.com/mrxz/aframe-locomotion/issues"
  },
  "homepage": "https://github.com/mrxz/aframe-locomotion#readme",
  "keywords": [
    "aframe",
    "vr",
    "xr",
    "webxr"
  ],
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@compodoc/live-server": "1.2.3",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/animejs": "3.1.0",
    "@types/three": "0.147.1",
    "aframe-types": "0.8.20",
    "aframe-typescript": "0.8.2",
    "aframe-typescript-docs-generator": "0.8.13",
    "concurrently": "^7.1.0",
    "handlebars": "^4.7.7",
    "rimraf": "^5.0.1",
    "rollup": "^2.71.1",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "tsup": "^6.7.0",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4"
  }
